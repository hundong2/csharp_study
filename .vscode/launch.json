        {
    "name": ".NET: Run Active C# file (dotnet run)",
    "type": "coreclr",
    "request": "launch",
    // dotnet CLI로 현재 파일을 실행 (dotnet run <file>)
    "program": "dotnet",
    "args": ["run", "${file}"],
    "cwd": "${workspaceFolder}",
    "console": "integratedTerminal",
    "stopAtEntry": false,
    "justMyCode": true
},
        {
            "name": ".NET Launch Active File",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${fileDirname}/bin/Debug/net8.0/${fileBasenameNoExtension}.dll",
            "args": [],
            "cwd": "${fileDirname}",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        },
            {
      "name": ".NET: Debug project of active file",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": ["run", "--project", "${fileDirname}"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "justMyCode": true
    },
        {
      "name": ".NET Launch (Example1)",
      "type": "dotnet",
